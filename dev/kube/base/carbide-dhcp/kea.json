{
  "Dhcp4": {
    "interfaces-config": {
      "interfaces": [
        "eth0"
      ],
      "dhcp-socket-type": "udp"
    },
    "lease-database": {
      "type": "memfile",
      "lfc-interval": 3600
    },
    "renew-timer": 900,
    "rebind-timer": 1800,
    "valid-lifetime": 3600,
    "hooks-libraries": [
      {
        "library": "/usr/lib/x86_64-linux-gnu/kea/hooks/libdhcp.so",
        "parameters": {
          "carbide-api-url": "http://carbide-api:1079",
          "carbide-provisioning-server-ipv4": "127.0.0.1"
        }
      }
    ],
    "subnet4": [
      {
        "subnet": "0.0.0.0/0",
        "pools": [
          {
            "pool": "0.0.0.0-255.255.255.255"
          }
        ]
      }
    ],
    "loggers": [
      {
        "name": "*",
        "output_options": [
          {
            "output": "stdout"
          }
        ],
        "severity": "DEBUG",
        "debuglevel": 10
      }
    ]
  }
}
