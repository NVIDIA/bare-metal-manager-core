graph "carbide" {
/*
 * AIR mgmt network is preconfigured to 192.168.200.0/24 and there does not 
 * appear to be a way of changing this
 */
  "fake" [function="fake"]
  "oob-mgmt-server" [function="oob-server" cpu="2" memory="2048"]
  "oob-mgmt-switch" [function="oob-switch" mgmt_ip="192.168.200.251"]
  "tor01" [function="leaf" version="5.0.0" mgmt_ip="192.168.200.10" ports="8"]

  "pxeclient" [ function="host" memory="1024" os="pxe" cpus="1"]
  "pxeserver" [ function="host" memory="1024" os="generic/ubuntu1804" cpus="1"]

  "oob-mgmt-server":"eth1" -- "oob-mgmt-switch":"swp2"
  "pxeserver":"eth0" -- "oob-mgmt-switch": "swp3"
  "pxeclient":"eth0" -- "oob-mgmt-switch": "swp4"
  "pxeserver":"eth1" -- "tor01":"swp1"
  "pxeclient":"eth1" -- "tor01":"swp2"

  # Internal Air connectivity for uncabled ports
  "air-internal" [function="fake"]
    "oob-mgmt-switch":"swp1" -- "air-internal":"eth1"
}
